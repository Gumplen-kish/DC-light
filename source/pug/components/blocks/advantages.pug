mixin advantages()
  .advantages
    .container.advantages__container
      +section-wrap("Наши преимущества", "")
      ul.advantages__list
        each elem, index in advantagesList
          li.advantages__item
            +icon(`icon-${elem.name}`, 64,64).advantages__icon
            .advantages__item-text
              h4=elem.title
              p=elem.text
      +wrap-text-border()